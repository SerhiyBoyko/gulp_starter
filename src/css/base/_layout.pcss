#page{
    min-height:$full;
}
.container{
    margin:0 auto;
    max-width:calc($desktop_res + 80px);
    width:$full;
    @media $m{
    &{
        padding:0 20px;
    }}
    @media $t{
    &{
        padding:0 30px;
    }}
    @media $d{
    &{
        padding:0 40px;
    }}
}

.relative{
    position:relative;
}